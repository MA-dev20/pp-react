      <thead>
        <tr>
          <th></th>
          <th>Company</th>
          <th>Department</th>
          <th>Team</th>
          <th>User (Self)</th>
          <th>None</th>
        </tr>
      </thead>
      <tbody>
        <%= form_for(:user_ability, url: update_ability_preset_path(params[:type], params[:abilities]), html: {id: "ability_form"}) do |f| %>
