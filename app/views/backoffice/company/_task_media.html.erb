<div class="card" id="task_media">
  <h3>Task Media</h3>
  <div class="table" id="task_media-table">
    <table>
      <thead>
        <tr>
          <th>Type</th>
          <th>Zeit</th>
        </tr>
      </thead>
      <tbody>
        <% @media.each do |m| %>
          <tr id="" class="medianame">
            <td><%= m.media_type %></td>
            <td><%= m.duration %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>
<script type="text/javascript">
  new PerfectScrollbar('#tasks-table');
  function redirect(tID) {
      var url = "<%= backoffice_company_url(@company) %>?pitch=" + tID;
      window.location.replace(url);
  }
</script>
